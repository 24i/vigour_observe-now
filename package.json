  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigour-io/observe-now.git"
  },
  "keywords": [
    "ploy",
    "now",
    "observe",
    "stream"
  ],
  "author": "Mustafa DokumacÄ±",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vigour-io/observe-now/issues"
  },
  "homepage": "https://github.com/vigour-io/observe-now#readme",
  "dependencies": {
    "JSONStream": "^1.0.0",
    "now": "^0.25.1",
    "vigour-observable": "^3.0.0"
  },
  "devDependencies": {
    "tape": "^4.4.0",
    "sinon": "^1.17.0",
    "ducktape": "^1.0.0",
    "tap-difflet": "0.4.0",
    "nodemon": "^1.9.1",
    "coveralls": "^2.11.9",
